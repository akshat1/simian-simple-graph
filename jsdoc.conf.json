{
  "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc","closure"]
  },
  "source": {
      "include": ["lib"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
      "plugins/markdown"
  ],
  "templates": {
      "cleverLinks": true,
      "monospaceLinks": true
  },
  "opts": {
      "destination": "docs",
      "readme": "README.md",
      "recurse": true,
      "template": "./node_modules/toast-jsdoc"
  }
}
